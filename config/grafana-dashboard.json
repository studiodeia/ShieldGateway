{
  "dashboard": {
    "id": null,
    "title": "GuardAgent Core - Sprint 2 Monitoring",
    "tags": ["guardagent", "security", "compliance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Request Latency (p95)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(guardagent_request_latency_ms_bucket[5m]))",
            "legendFormat": "p95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 200},
                {"color": "red", "value": 300}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(guardagent_requests_total[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Authentication Failures",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(guardagent_auth_failures_total[5m])",
            "legendFormat": "Auth Failures/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Blocking Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "guardagent_blocking_rate",
            "legendFormat": "Blocking Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "Request Latency Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(guardagent_request_latency_ms_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(guardagent_request_latency_ms_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(guardagent_request_latency_ms_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "yAxes": [
          {
            "label": "Latency (ms)",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Queue Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "guardagent_queue_size{status=\"waiting\"}",
            "legendFormat": "Waiting"
          },
          {
            "expr": "guardagent_queue_size{status=\"active\"}",
            "legendFormat": "Active"
          },
          {
            "expr": "guardagent_queue_size{status=\"failed\"}",
            "legendFormat": "Failed"
          }
        ],
        "yAxes": [
          {
            "label": "Queue Size",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Security Events",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(guardagent_security_events_total{blocked=\"true\"}[5m])",
            "legendFormat": "Blocked Events/sec"
          },
          {
            "expr": "rate(guardagent_security_events_total{blocked=\"false\"}[5m])",
            "legendFormat": "Detected Events/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Events/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "WORM Logs Status",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(guardagent_worm_logs_total{status=\"success\"}[5m])",
            "legendFormat": "Success/sec"
          },
          {
            "expr": "rate(guardagent_worm_logs_total{status=\"error\"}[5m])",
            "legendFormat": "Errors/sec"
          }
        ],
        "yAxes": [
          {
            "label": "Logs/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 9,
        "title": "API Key Usage by Tenant",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, rate(guardagent_api_key_usage_total[5m]))",
            "format": "table",
            "instant": true
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 24}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1
  }
}
