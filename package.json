{
  "name": "guardagent-core",
  "version": "0.1.0",
  "description": "AI Security Gateway - LGPD/GDPR compliant with WORM logging",
  "keywords": [
    "ai",
    "security",
    "gateway",
    "llm",
    "firewall",
    "lgpd",
    "gdpr",
    "compliance"
  ],
  "license": "MIT",
  "homepage": "https://github.com/guardagent/core",
  "author": {
    "name": "GuardAgent",
    "email": "info@guardagent.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardagent/core.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts --quiet src",
    "lintfix": "eslint --ext .ts --quiet --fix src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:fuzz": "npm run build && node scripts/fuzz-test.js",
    "benchmark": "npm run build && node scripts/benchmark.js",
    "check-performance": "node scripts/check-benchmark-threshold.js 150",
    "perf:test": "mkdir -p perf/results && k6 run perf/guard_100rps.js",
    "perf:test-300rps": "mkdir -p perf/results && k6 run perf/guard_300rps.js",
    "perf:test-local": "BASE_URL=http://localhost:8080 npm run perf:test",
    "perf:test-300rps-local": "BASE_URL=http://localhost:8080 npm run perf:test-300rps",
    "docker:build": "docker build -t guardagent/core:latest .",
    "docker:run": "docker run -p 8080:8080 guardagent/core:latest",
    "cli:keys": "tsx src/cli/keys.ts",
    "worker:logs": "tsx src/workers/logWorker.ts",
    "cron:rotate-keys": "tsx src/cron/rotateKeys.ts",
    "cron:rotate-keys-trigger": "tsx src/cron/rotateKeys.ts --trigger"
  },
  "files": [
    "dist",
    "config"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.5",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonpath": "^0.2.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "~5.3.0"
  },
  "dependencies": {
    "@aws-sdk/client-kms": "^3.470.0",
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/client-sqs": "^3.470.0",
    "@sendgrid/mail": "^8.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node-jose": "^1.1.13",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^4.15.0",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "jsonpath": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "node-cron": "^3.0.3",
    "node-jose": "^2.2.0",
    "pg": "^8.11.3",
    "pino": "^8.17.0",
    "pino-pretty": "^10.3.0",
    "prom-client": "^15.1.0",
    "reflect-metadata": "^0.1.13",
    "stripe": "^18.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "ulid": "^2.3.0",
    "ws": "^8.14.2",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  }
}
