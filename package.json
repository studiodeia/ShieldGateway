{
  "name": "n8n-nodes-shield-gateway",
  "version": "0.1.0",
  "description": "LLM Firewall para proteger fluxos de IA no n8n",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node",
    "llm",
    "security",
    "firewall"
  ],
  "license": "MIT",
  "homepage": "https://github.com/shield-gateway/n8n-nodes-shield-gateway",
  "author": {
    "name": "ShieldGateway",
    "email": "info@shieldgateway.io"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shield-gateway/n8n-nodes-shield-gateway.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint --ext .ts --quiet src",
    "lintfix": "eslint --ext .ts --quiet --fix src",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:fuzz": "npm run build && node scripts/fuzz-test.js",
    "benchmark": "npm run build && node scripts/benchmark.js",
    "check-performance": "node scripts/check-benchmark-threshold.js 30"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/ShieldGateway/ShieldGateway.node.js"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "n8n": ">=1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^18.18.0",
    "@types/request-promise-native": "^1.0.19",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "n8n-workflow": "~1.19.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^8.5.0",
    "jsonpath": "^1.1.1",
    "knex": "^2.5.1",
    "n8n-core": "~1.19.0",
    "re2-wasm": "^1.0.4"
  }
} 